	<DepsBuiltPath>$(DepsPath)lib\$(SimpleConfiguration)\$(PlatformName)\$(PlatformToolset)\</DepsBuiltPath>
	<DepLibsPath>
	$(DepsBuiltPath);
	$(DepsPath)OpenCL\lib\$(PlatformName)\;
	$(DepsPath)libiomp\lib\$(PlatformName)\
	</DepLibsPath>
	<!-- libraries -->
	<CadlibLib>AVCadLib.lib</CadlibLib>
	<CryptoppLib>cryptlib.lib</CryptoppLib>
	<CurlLib>curl.lib</CurlLib>
	<ExifLib>lib_exif.lib</ExifLib>
